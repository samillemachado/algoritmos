// Crie um algoritmo que leia um número real e mostre na tela o seu dobro e a sua terça parte.

algoritmo "P01 - E7"
var
   input, dobro, terca: real
inicio
      escreva ("Digite um número: ")
      leia (input)
      
      dobro <- input*2
      terca <- input/3
      escreval ("Seu dobro é: ", dobro)
      escreval ("Sua terça parte é: ", terca:4:1)

fimalgoritmo
